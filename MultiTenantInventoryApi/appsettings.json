{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TenantSettings": {
    "alpha-logistics": {
      "EnableCheckout": true,
      "MaxItemsPerUser": 3,
      "AllowedItemCategories": [ "Tools", "Machinery" ],
      "Features": {
        "AdvancedSearch": true,
        "ExportToExcel": false,
        "SpecialReport": true
      }
    },
    "beta-supply": {
      "EnableCheckout": true,
      "MaxItemsPerUser": 5,
      "AllowedItemCategories": [ "Electronics","Safety" ],
      "Features": {
        "AdvancedSearch": false,
        "ExportToExcel": true
      }
    }
  }
}
